{
    "env": {
        "browser": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "semi": "error",
        "curly": "error",
        "quotes": [
            "warn",
            "single"
        ],
        "no-undef": "error"
    }
}